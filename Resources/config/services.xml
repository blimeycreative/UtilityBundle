<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="oxygen_pagination.class">Oxygen\PaginationBundle\Paginate</parameter>
        <parameter key="oxygen_pagination.factory.class">Oxygen\PaginationBundle\Factory</parameter>
    </parameters>

    <services>
        <service id="oxygen_pagination.factory" class="%oxygen_pagination.factory.class%">
            <argument type="service" id="service_container" />
            <argument>%oxygen_pagination.class%</argument>
        </service>
    </services>
</container>
